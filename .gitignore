# Ignore Drupal file directory
docroot/sites/*/files
docroot/sites/*/files-private
docroot/sites/*/private

# Ignore environment-specific settings
docroot/sites/*/settings.local.php
docroot/sites/*/services*.yml
docroot/sites/development.services.yml
!docroot/sites/*/services.yml

# Ignore logs and databases
*.log
*.sql
db/**
!db/readme.txt

# Ignore files generated by text editors
.idea
.vscode

# Ignore OS generated files
.DS_Store*
ehthumbs.db
Thumbs.db
._*

# Ignore SASS cache
.sass-cache

# Ignore some Composer generated content
vendor/drupal/coder/
vendor/behat/
# @TODO: This seems questionable
docroot/modules/contrib/devel/
# We're tracking the vendor directory so we don't have to run
# composer install on server. But there's some files we want to ignore
# to avoid merge conflicts.
vendor/composer/installed.php
vendor/composer/InstalledVersions.php
vendor/composer/installed.json
